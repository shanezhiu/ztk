{
    "name": "tenet/ztk",
    "description": "折淘客平台SDK",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "淘宝客", "折淘客", "SDK"
    ],
    "authors": [
        {
            "name": "shanezhiu",
            "email": "shanezhiu@gmail.com"
        }
