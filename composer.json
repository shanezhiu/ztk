        "php-di/php-di": "^6.2",
        "monolog/monolog": "^2.1"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "^0.12",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-rest":"*",
        "mockery/mockery": "^1.4@dev"
